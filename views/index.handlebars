<div class="album py-5 bg-light">
    <div class="container">
        <div class="row">
            {{#each materias}}
            <!-- for each loop in handlebars -->
            <div class="col-md-4">
                <div class="card mb-4 box-shadow">
                    
                    <a href={{url}}.>
                        <img class="card-img-top filethumb" src= {{image}} alt="Card image cap">
                    </a>

                    <div class="card-body">
                        <p class="card-text"></p>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="btn-group">
                                <button type="button" class="btn btn-sm btn-outline-secondary">{{text}}.</button>
                            </div>
                            <small class="text-muted">9 mins</small>
                        </div>
                    </div>

                </div>
            </div>
            {{else}} {{/each}}
        </div>
    </div>
</div>